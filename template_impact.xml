<quakeml xmlns="urn:Test.Namespace">
  <eventParameters>
    <event publicID="quakeml:us.anss.org/event/[ID]">
      <attachments>
        <uri>
          [DATASOURCE]
        </uri>
        <comment>
          [COMMENT]
        </comment>
        <creationInfo>
          <agencyID>us
          </agencyID>
          <author>[AUTHOR]
          </author>
          <creationTime>[CTIME]
          </creationTime>
        </creationInfo>
      </attachments>
      
      <origin publicID="quakeml:us.anss.org/impact/[ID]">
        <time>
          <value>[TIME]
          </value>
        </time>
        <latitude>
          <value>
            [LAT]
          </value>
        </latitude>
        <longitude>
          <value>
            [LON]
          </value>
        </longitude>
        <depth>
          <value>
            [DEPTH]
          </value>
        </depth>
      </origin> 
      <!-- Effects Section -->
      <effects comment="[TSUNAMI_COMMENT]" effectsFlag="Tsunami"/>
      <effects comment="[SEICHE_COMMENT]" effectsFlag="Seiche"/>
      <effects comment="[MINECOLLAPSE_COMMENT]" effectsFlag="MineCollapse"/>
      <effects comment="[SANDBLOW_COMMENT]" effectsFlag="SandBlow"/>
      <effects comment="[UPLIFT_COMMENT]" effectsFlag="Uplift"/>
      <effects comment="[SUBSIDENCE_COMMENT]" effectsFlag="Subsidence"/>
