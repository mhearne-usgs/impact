      <effects comment="[FIRE_COMMENT]" effectsFlag="Fire"/>
      <effects comment="[VOLCANIC_COMMENT]" effectsFlag="VolcanicActivity"/>
      <effects comment="[GEYSER_COMMENT]" effectsFlag="GeyserActivity"/>
      <effects comment="[SOUNDS_COMMENT]" effectsFlag="Sounds"/>
      <effects comment="[OTHER_COMMENT]" effectsFlag="Other"/>
      <!-- End of Effects Section -->
      
      <!-- Causes Section -->
      <causes comment="[TSUNAMI_COMMENT]" causesFlag="Tsunami"/>
      <causes comment="[SEICHE_COMMENT]" causesFlag="Seiche"/>
