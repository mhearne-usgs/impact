      <causes comment="[GEYSER_COMMENT]" causesFlag="GeyserActivity"/>
      <causes comment="[SOUNDS_COMMENT]" causesFlag="Sounds"/>
      <causes comment="[OTHER_COMMENT]" causesFlag="Other"/>
